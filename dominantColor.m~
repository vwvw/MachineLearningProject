function [  ] = dominantColor( data,i )

h = fspecial('average',3);%bluring
A = getImage(data, i);
blur = imfilter(A, h, 'replicate');
figure
image(blur)
dominantColor = [0,0,0];
dominantColorOccurences = -Inf;
for i = 1 : 4
    for j = 1 : 4
        if mod(i+j,2) == 0
           R = A(i,j,1);
           G = A(i,j,2);
           B = A(i,j,3);
           
               counter = 0;
               for k = 1 : 32
                   for l = 1 : 32
                       r =abs(R- A(k,l,1));
                       g = abs(G- A(k,l,2));
                       b = abs(B- A(k,l,3));
                       u=r+g+b;
                        if u  < 1
                            counter= counter +1;
                        end
                   end
               end
               if(counter > dominantColorOccurences)
                   dominantColorOccurences = counter;
                   dominantColor = [R,G,B];
               end
           
        end
    end
end

result = zeros(32,32,3);
for i = 1 : 32
    for j = 1 : 32
       for k = 1 : 3
           %result(i,j,k) = dominantColor(1,k);
           result(i,j,1) = 255;
           result(i,j,2) = 128;
       end
    end
end
figure
image(result)
end

